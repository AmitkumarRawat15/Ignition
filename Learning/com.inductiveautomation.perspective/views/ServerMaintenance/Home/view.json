            "propConfig": {
              "props.value": {
                "onChange": {
                  "enabled": null,
                  "script": "\t\n\t#Number of Designers Open\n    session_filter \u003d {\"clientType\": \"designer\"}\t\t    \n    session_info \u003d system.util.getSessionInfo(sessionFilter\u003dsession_filter)\t\t  \n    if session_info is not None:\t\t        \n        active_session_count \u003d len(session_info)\t\n        self.parent.getChild(\"cntDesigner\").getChild(\"lblDesignerConnected\").props.text\u003d str(active_session_count)+\" \"+str(\"Open\")       \n    else:\t        \n    \tself.parent.getChild(\"cntDesigner\").getChild(\"lblDesignerConnected\").props.text\u003d \"0\"+\" \"+str(\"Open\")     \n    \n    #OPCServers\n    notconnectedcount\u003d0\n    allOPCServers \u003d system.opc.getServers()\n    OPCServerCount\u003dlen(allOPCServers)\n    for server in allOPCServers:\n    \tstatus\u003dsystem.opc.getServerState(server)\n    \tif status !\u003d \"CONNECTED\":\n    \t\tnotconnectedcount\u003dnotconnectedcount+1\n    connected\u003dOPCServerCount-notconnectedcount\n    self.parent.getChild(\"cntOPC\").getChild(\"lblOPCConnected\").props.text \u003d str(connected)+ \"/\"+str(OPCServerCount)+\" \"+\"Connected\"\n    \n    #Perspective Sessions\n    sessions \u003d system.perspective.getSessionInfo()\n    clientcount\u003d0  \n    for session in sessions:\n    \tclient\u003d session.get(\"sessionScope\")\n    \tif client !\u003d \u0027designer\u0027:\n    \t\tclientcount\u003dclientcount+1\n    self.parent.getChild(\"cntPerspective\").getChild(\"lblPerspectiveConnected\").props.text \u003d str(clientcount)+\" \"+str(\"Open\") \n    \n    \n    #Database\n    connections \u003d system.db.getConnections()\n    connections\u003dsystem.dataset.toPyDataSet(connections)\n    totalconnected \u003d len(connections)\n    active\u003d0\n    for row in range(len(connections)):\n        connection_status \u003d connections[row][\u0027Status\u0027]\n        if connection_status:\n            active\u003dactive+1\n    self.parent.getChild(\"cntDatabase\").getChild(\"lblDatabaseConnected\").props.text \u003d str(active)+ \"/\"+ str(totalconnected)+\" \"+\"connected\"\n    \n    #Devices\n    device \u003d system.device.listDevices()\n    device\u003dsystem.dataset.toPyDataSet(device)\t\n    devicecount\u003dlen(device)\n    self.parent.getChild(\"cntDevices\").getChild(\"lblDevicesConnected\").props.text \u003d str(devicecount)+\" \"+\"enabled\"\n    \n    #Gateway Network Servers\n    allServers \u003d system.net.getRemoteServers()\n    runningServers \u003d system.net.getRemoteServers(True)\n    allServers\u003dlen(allServers)\n    runningServers\u003dlen(runningServers)\n    self.parent.getChild(\"cntGatewayNetworkconnections\").getChild(\"lblGatewayconnected\").props.text \u003d str(runningServers)+\u0027/\u0027+str(allServers )+\u0027 \u0027+\u0027connections\u0027\n    \n    #Store and Forward\n    \u0027\u0027\u0027\n    activities \u003d system.util.getSFCachedMessages()\n    activities \u003d len(activities)\n    self.parent.getChild(\"cntStoreForward\").getChild(\"lblStorequarantied\").props.text \u003d str(activities)\n    \u0027\u0027\u0027"
                }
              }
